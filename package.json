{
  "name": "tscripter",
  "version": "0.1.2",
  "description": "Typescript code generation tools.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/typescript/bin/tsc && ./node_modules/mocha/bin/mocha",
    "test-fast": "./node_modules/typescript/bin/tsc && ./node_modules/mocha/bin/mocha --grep \"@slow\"",
    "test-slow": "./node_modules/typescript/bin/tsc && ./node_modules/mocha/bin/mocha --grep \"@slow\"",
    "download-typescript-tests": "./test/download-typescript-tests",
    "format-tests": "find test -name \"*.test.ts\" | grep \"\\.ts\" | xargs ./node_modules/typescript-formatter/bin/tsfmt -r",
    "format-lib": "find lib | grep \"\\.ts\" | xargs ./node_modules/typescript-formatter/bin/tsfmt -r",
    "format-index": "./node_modules/typescript-formatter/bin/tsfmt -r index.ts",
    "format": "npm run format-tests && npm run format-lib && npm run format-index",
    "publish-clean": "./publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corps/typescripter.git"
  },
  "author": "recursive.cookie.jar@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/corps/typescripter/issues"
  },
  "homepage": "https://github.com/corps/typescripter",
  "dependencies": {
    "typescript": "^1.5.0-beta"
  },
  "devDependencies": {
    "typescript-formatter": "^0.3.2",
    "mocha": "^2.2.4"
  }
}
